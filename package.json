{
  "name": "mortgage-options",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/martucazpo/Mortgage-Options.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/martucazpo/Mortgage-Options/issues"
  },
  "homepage": "https://github.com/martucazpo/Mortgage-Options#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.2",
    "express": "^4.17.1",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "materialize": "^1.0.0",
    "materialize-css": "^1.0.0",
    "mongoose": "^5.7.1",
    "mortgage-calculator-react": "^0.1.3",
    "mortgage-js": "^0.1.2",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "react-google-charts": "^3.0.15",
    "validator": "^11.1.0"
  },
  "devDependencies": {
    "nodemon": "^1.19.2"
  },
  "description": ""
}
